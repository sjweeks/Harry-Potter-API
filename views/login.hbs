<div class="contentContainer">
    <h1>Login</h1>
    <ul>
        {{#each data}}
        <li>{{@key}} : {{this}}</li>
        {{/each}}
    </ul>
    <br>
    <div class="login">
        <form action="/login" method="POST">
            <div>
                <input class="emailAddress" type="text" name="username" placeholder="Enter Username">
            </div>
            <br>
            <div>
                <input class="password" type="password" name="password" placeholder="Enter Password">
            </div>

            <br>
            <br>
            <input class="inputButton" type="submit" value="Submit">
        </form>
    </div><br>
<h3>{{err}}</h3>

</div>